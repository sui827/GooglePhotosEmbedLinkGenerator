<%-include('components/header')%>

<nav class="navbar navbar-expand-lg navbar-dark bg-dark text-white">
  <a class="navbar-brand" href="/"><%=title%></a>
</nav>

<main class="container-lg">
  <%-include('components/form')%>
  <div class="row">
    <%for(let link of links){%>
      <div class="photo-card col-6 col-md-4 col-lg-3">
        <div class="img-box">
          <img src="<%=link%>=w240">
        </div>
      </div>
    <%}%>
  </div>
</main>

<style>
  .photo-card {
    
  }
  .img-box {
    width: 100%;
    height: 100%;
    position: relative;
  }
  .img-box::before {
    content: "";
    padding-top: 100%;
  }
  .img-box img {
    width: 100%;
    height: auto;
    line-height: 0;
    object-fit: contain;
    position: absolute;
    bottom: 0;
  }
</style>


<%-include('components/footer')%>